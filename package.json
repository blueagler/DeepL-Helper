    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "0.4.3",
    "@types/babel__core": "^7.20.1",
    "@types/random-useragent": "^0.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "babel-plugin-transform-imports": "^2.0.0",
    "esbuild": "^0.18.2",
    "rollup": "3.29.5",
