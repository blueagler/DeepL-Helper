    "@mui/x-data-grid-premium": "6.7.0",
    "@mui/x-license-pro": "6.6.0",
    "byte-guide": "^1.0.7",
    "jszip": "^3.10.1",
    "localforage": "^1.10.0",
    "mobx": "^6.9.0",
    "mobx-persist-store": "^1.1.3",
    "mobx-react": "^8.0.0",
    "notistack": "3.0.1",
    "preact": "^10.15.1",
    "pretty-bytes": "^6.1.0",
    "random-useragent": "0.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
